<h1>User Profiles</h1>

<table>
  <thead>
    <tr>
      <th>Username</th>
      <th></th>
      <th>Age</th>
      <th></th>
      <th>Gender</th>
      <th></th>
      <th>Streetaddress</th>
      <th></th>
      <th>City</th>
      <th></th>
      <th>State</th>
      <th></th>
      <th>Zip</th>
      <th></th>
      <th>Profilepicurl</th>
      <th></th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_profiles.each do |user_profile| %>
      <tr>
        <td><%= user_profile.userName %></td>
        <td><%= user_profile.age %></td>
        <td><%= user_profile.gender %></td>
        <td><%= user_profile.streetAddress %></td>
        <td><%= user_profile.City %></td>
        <td><%= user_profile.State %></td>
        <td><%= user_profile.Zip %></td>
        <td><%= user_profile.profilePicUrl %></td>
        <td><%= link_to 'Show', user_profile %></td>
        <td><%= link_to 'Edit', edit_user_profile_path(user_profile) %></td>
        <td><%= link_to 'Destroy', user_profile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User profile', new_user_profile_path %>
